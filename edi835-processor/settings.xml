    </profiles>

    <!-- Active Profiles (uncomment to activate specific profiles) -->
    <activeProfiles>
        <activeProfile>default-repos</activeProfile>
        <!-- <activeProfile>corporate-artifactory</activeProfile> -->
        <!-- <activeProfile>azure-artifacts</activeProfile> -->
    </activeProfiles>

    <!-- Proxies (if behind corporate firewall) -->
