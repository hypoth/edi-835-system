                </pluginRepository>
            </pluginRepositories>
        </profile>
        -->

        <!-- Azure Artifacts Profile (for Azure DevOps Maven feeds) -->
        <!--
        <profile>
            <id>azure-artifacts</id>
            <repositories>
                <repository>
                    <id>azure-artifacts-feed</id>
                    <url>https://pkgs.dev.azure.com/your-org/_packaging/your-feed/maven/v1</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </repository>
            </repositories>
        </profile>
